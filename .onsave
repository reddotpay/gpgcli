#!/usr/bin bash

# Generates compiled executables 

function main {

    mkdir -p dist/win
    mkdir -p dist/linux

    local F=$(basename `git config --get remote.origin.url` | cut -f 1 -d '.')

    GOOS=windows GOARCH=386 go build -o dist/win/$F.exe .
    GOOS=linux GOARCH=amd64 go build -o dist/linux/$F .
}

main